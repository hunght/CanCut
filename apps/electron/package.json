{
  "name": "@learnifytube/electron",
  "productName": "CanCut",
  "version": "1.0.28",
  "description": "CanCut - A powerful video editor application with a beautiful modern interface.",
  "main": ".vite/build/main.js",
  "packageManager": "bun@1.2.18",
  "scripts": {
    "rebuild": "./node_modules/.bin/electron-rebuild; bun rebuild",
    "clear": "rm -rf .vite dist out node_modules",
    "clean": "rm -rf .vite dist out",
    "restart": "./scripts/restart.sh --start",
    "type-check": "bunx --bun tsc --noEmit",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "lint:complexity": "eslint . --format json | node scripts/complexity-report-html.cjs",
    "dev": "electron-forge start",
    "dev:electron": "electron-forge start",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "win-make": "bun run clean:out && bun run debug-make",
    "win-build": "cross-env PLATFORM=win32 bun run make",
    "mac-build": "cross-env PLATFORM=darwin bun run make",
    "clean:out": "powershell -Command \"Stop-Process -Name CanCut -Force -ErrorAction SilentlyContinue; Start-Sleep -Seconds 2; Remove-Item -Path out -Force -Recurse -ErrorAction SilentlyContinue\"",
    "publish": "electron-forge publish",
    "prett": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest",
    "test:database": "jest --config jest.database.config.ts",
    "test:database:watch": "jest --config jest.database.config.ts --watch",
    "test:e2e": "playwright test",
    "test:all": "jest && playwright test",
    "screenshots": "./scripts/screenshots-scripts/generate-screenshots.sh",
    "screenshots:verbose": "./scripts/screenshots-scripts/generate-screenshots.sh --verbose",
    "screenshots:quick": "./scripts/screenshots-scripts/generate-screenshots.sh --skip-build",
    "release": "./scripts/release.sh",
    "debug-make": "DEBUG=electron-forge:* bun run make",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "db:reset": "rm -f local.db* && bun run db:apply",
    "db:backup": "bunx --bun tsx scripts/db-backup.ts",
    "db:health": "bunx --bun tsx scripts/db-health.ts",
    "db:status": "bunx --bun tsx scripts/db-status.ts",
    "db:counts": "bunx --bun tsx scripts/db-counts.ts",
    "db:vacuum": "bunx --bun tsx scripts/db-vacuum.ts",
    "db:analyze": "bunx --bun tsx scripts/db-analyze.ts",
    "icons:generate": "node scripts/svg-to-icons.js",
    "postinstall": "lefthook install || true"
  },
  "author": "HungHoang <hth321@gmail.com>",
  "license": "MIT",
